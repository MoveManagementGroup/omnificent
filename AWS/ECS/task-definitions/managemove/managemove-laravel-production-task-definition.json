{
  "family": "managemove-laravel-production-task-definition",
  "volumes": [
    {
      "host": {
        "sourcePath": "/home/ec2-user/mmginc-web-settings/production/managemove-laravel/.env"
      },
      "name": "ManageMoveVolume"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "mmginc/managemove-laravel:%REVISION%",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "mountPoints": [
        {
          "sourceVolume": "ManageMoveVolume",
          "containerPath": "/var/www/html/.env"
        }
      ],
      "name": "ManageMoveLaravel"
    },
    {
      "essential": true,
      "image": "mmginc/managemove-nginx:latest",
      "links": [
        "ManageMovePhpFpm"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "lg-managemove-laravel-production",
          "awslogs-region": "us-west-2"
        }
      },
      "memory": 100,
      "name": "ManageMoveNginx",
      "portMappings": [
        {
          "hostPort": 60000,
          "containerPort": 80
        }
      ],
      "volumesFrom": [
        {
          "sourceContainer": "ManageMoveLaravel"
        }
      ]
    },
    {
      "essential": true,
      "image": "mmginc/managemove-php-fpm:latest",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 200,
      "name": "ManageMovePhpFpm",
      "volumesFrom": [
        {
          "sourceContainer": "ManageMoveLaravel"
        }
      ]
    }
  ]
}