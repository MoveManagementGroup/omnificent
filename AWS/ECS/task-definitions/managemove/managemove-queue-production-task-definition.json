{
  "family": "managemove-queue-production-task-definition",
  "volumes": [
    {
      "host": {
        "sourcePath": "/home/ec2-user/mmginc-web-settings/production/managemove-laravel/.env"
      },
      "name": "ManageMoveVolume"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "mmginc/managemove-laravel:latest",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "mountPoints": [
        {
          "sourceVolume": "ManageMoveVolume",
          "containerPath": "/var/www/html/.env"
        }
      ],
      "name": "ManageMoveLaravel"
    },
    {
      "command": [
        "php",
        "artisan",
        "queue:listen",
        "database",
        "--timeout=10800",
        "--tries=1",
        "--verbose"
      ],
      "essential": true,
      "image": "mmginc/managemove-queue:%REVISION%",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "lg-managemove-queue-production",
          "awslogs-region": "us-west-2"
        }
      },
      "memory": 850,
      "name": "ManageMoveQueue",
      "volumesFrom": [
        {
          "sourceContainer": "ManageMoveLaravel"
        }
      ]
    }
  ]
}